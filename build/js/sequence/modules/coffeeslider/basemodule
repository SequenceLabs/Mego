(function() {
  "use strict";
  var modules,
    __bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  modules = SEQ.utils.namespace('SEQ.modules');

  modules.BaseModule = (function() {

    function BaseModule(options) {
      this.options = options;
      this.registerNavModule = __bind(this.registerNavModule, this);
      this.goTo = __bind(this.goTo, this);
      this.init = __bind(this.init, this);
      this.currentIndex = 0;
      this.element = {};
      this.active = {};
      this.navModules = [];
      this.init();
    }

    BaseModule.prototype.init = function() {
      return this.goTo(0);
    };

    BaseModule.prototype.goTo = function(index, skipTransition) {
      var navModule, _i, _len, _ref, _results;
      this.currentIndex = index;
      _ref = this.navModules;
      _results = [];
      for (_i = 0, _len = _ref.length; _i < _len; _i++) {
        navModule = _ref[_i];
        if (navModule.goTo != null) {
          _results.push(navModule.goTo(index, skipTransition));
        } else {
          _results.push(void 0);
        }
      }
      return _results;
    };

    BaseModule.prototype.registerNavModule = function(navModule) {
      var _this = this;
      navModule.element.on("change", function(e) {
        return _this.goTo(navModule.currentIndex);
      });
      return this.navModules.push(navModule);
    };

    return BaseModule;

  })();

}).call(this);
